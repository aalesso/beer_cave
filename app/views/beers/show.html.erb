<div class="page-header">
  <h3>Beer #<%= @beer.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Category</dt>
      <dd><%= @beer.category %></dd>

      <dt>Name.string</dt>
      <dd><%= @beer.name.string %></dd>

      <dt>User</dt>
      <dd><%= @beer.user_id %></dd>

      <dt>Fellow imbibers</dt>
      <dd><%= @beer.fellow_imbibers %></dd>

      <dt>Favorite</dt>
      <dd><%= @beer.favorite %></dd>

      <dt>Year</dt>
      <dd><%= @beer.year %></dd>

      <dt>Rating</dt>
      <dd><%= @beer.rating %></dd>

      <dt>Notes</dt>
      <dd><%= @beer.notes %></dd>

      <dt>Grape</dt>
      <dd><%= @beer.grape %></dd>

      <dt>Country</dt>
      <dd><%= @beer.country %></dd>

      <dt>Photo</dt>
      <dd><%= @beer.photo %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/beers" class="btn btn-primary">
        Back
      </a>
      <a href="/beers/<%= @beer.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_beer/<%= @beer.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
